  "author": {
    "name": "Chaillou Jean-Yves",
    "email": "jeanyves.chaillou@gmail.com",
    "url": "https://kwabounga.fr"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kwabounga/publics-apis-app.git"
  }
}
